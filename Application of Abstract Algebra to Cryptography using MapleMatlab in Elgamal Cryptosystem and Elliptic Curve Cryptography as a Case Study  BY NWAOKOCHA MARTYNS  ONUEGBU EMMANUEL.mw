<Group labelreference="L155" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">epoints := proc(ec, x, ub, p)  #the third parameter ub stands for &quot;upper boundary&quot;
local ecurve, z, pct, k, i;
pct := 0;
for k from 0 to p-1 while pct &lt;= ub do
z := subs(x=k, ec) mod p;
if z = 0 then
pct := pct+1;
ecurve[pct] := [k,z];
fi:
if z &amp;^ ((p-1)/2) mod p = 1 then
z := z &amp;^ ((p+1)/4) mod p;
ecurve[pct+1] := [k,z];
ecurve[pct+2] := [k, -z mod p];
pct := pct+2;
fi:
od:
if pct &gt; ub then
pct := ub:
fi:
seq(ecurve[i], i=1..pct):
end:</Text-field>
</Input>
</Group>
