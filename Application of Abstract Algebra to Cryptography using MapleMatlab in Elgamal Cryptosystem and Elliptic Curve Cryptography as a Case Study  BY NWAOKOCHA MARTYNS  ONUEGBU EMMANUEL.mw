<Group labelreference="L154" drawlabel="true" applyint="true" applyrational="true" applyexponent="false">
<Input><Text-field prompt="&gt; " style="Maple Input" layout="Normal">tonumber := proc(imess)
local sl, cn, cnl, mess, sn, ii, j, ntable;
ntable := table([&quot;A&quot;=0, &quot;B&quot;=1, &quot;C&quot;=2, &quot;D&quot;=3, &quot;E&quot;=4,
&quot;F&quot;=5, &quot;G&quot;=6, &quot;H&quot;=7, &quot;I&quot;=8, &quot;J&quot;=9, &quot;K&quot;=10, &quot;L&quot;=11,
&quot;M&quot;=12, &quot;N&quot;=13, &quot;O&quot;=14, &quot;P&quot;=15, &quot;Q&quot;=16, &quot;R&quot;=17, &quot;S&quot;=18,
